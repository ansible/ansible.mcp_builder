---
{% for registry in registries %}
- name: {{ registry.name }}
  servers:
  {% for server in registry.registry %}
    - repository:
        {% for key, value in server[0].items() %}
        {{key}}: {{ value }}
        {% endfor %}
      variables:
        {% for key, value in server[1].items() %}
        {{key}}: {{ value }}
        {% endfor %}
  {% endfor %}
{% endfor %}
